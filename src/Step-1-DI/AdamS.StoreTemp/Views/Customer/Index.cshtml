@model System.Collections.Generic.List<AdamS.StoreTemp.Models.Customer>

@{
    ViewBag.Title = "title";

}

<h2>Customers</h2>


<table class="table table-striped">
    
    <tr class="h4">
        <th>Id</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th></th>

    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.Id</td>
            <td>@customer.FirstName</td>
            <td>@customer.LastName</td>
            <td>@Html.ActionLink("Delete","Delete", new {id=customer.Id})</td>

        </tr>



    }
</table>